{
  "manifest_version": 2,
  "name": "Get Gift",
  "version": "1.1",
  "description": "Get your Twitch gift channel points automatically.",
  "author": "RBalconi",
  "content_scripts": [{
    "matches": ["https://www.twitch.tv/*"],
    "css": ["styles/styles.min.css"],
    "js": ["scripts/scripts.min.js"]
  }],
  "web_accessible_resources": [
    "assets/*.svg",
    "assets/beep.mp3"
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  }
}